<%- include("../partials/header") %>
<div class="page">
    <div class="book">
        <%books.forEach(book=>{%>
        <div>
            <a href="/books/<%-book.id%>"><img class="bookImg"src=<%- book.imgUrl%> alt="book"></a>
            <div class="textSpace">
                <h3><%- book.title%></h3>
                <p><%- book.author%></p>
            </div>
    </div>
        <%})%>
    </div>
</div>
<%- include("../partials/footer") %>